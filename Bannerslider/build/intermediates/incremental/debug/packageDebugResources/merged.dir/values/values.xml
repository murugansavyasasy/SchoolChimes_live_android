<?xml version="1.0" encoding="utf-8"?>
<resources>
    <color name="default_indicator_color_selected">#eeee</color>
    <color name="default_indicator_color_unselected">#66eeeeee</color>
    <dimen name="default_dash_indicator_width">12dp</dimen>
    <dimen name="default_indicator_margins">5dp</dimen>
    <dimen name="default_indicator_size">8dp</dimen>
    <dimen name="max_slider_indicator_size">16dp</dimen>
    <dimen name="round_square_indicator_radius">2dp</dimen>
    <string name="app_name">BannerSlider</string>
    <declare-styleable name="Slider">
        <attr format="boolean" name="slider_hideIndicators"/>
        <attr format="dimension" name="slider_indicatorSize"/>
        <attr format="reference" name="slider_selectedSlideIndicator"/>
        <attr format="reference" name="slider_unselectedSlideIndicator"/>
        <attr format="boolean" name="slider_animateIndicators"/>
        <attr format="integer" name="slider_interval"/>
        <attr format="boolean" name="slider_loopSlides"/>
        <attr format="integer" name="slider_defaultBanner"/>
        <attr format="reference" name="slider_emptyView"/>
    </declare-styleable>
</resources>